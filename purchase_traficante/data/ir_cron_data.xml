<odoo>

    <data noupdate = "1">

        <record id = "update_customer_type_from_orders" model = "ir.cron">
            <field name = "name">Obtener la fecha de recepcion para los pedidos de compras</field>
            <field name = "model_id" ref = "purchase.model_purchase_order_line" />
            <field name="active" eval="True"></field>
            <field name = "state">code</field>
            <field name = "code">
lines = env['purchase.order.line'].search([])

for line in lines:
  line._get_received_date()
            </field>
            <field name = "user_id" ref = "base.user_root" />
            <field name = "interval_number">1</field>
            <field name = "interval_type">days</field>
            <field name = "numbercall">-1</field>
            <field name = "doall" eval = "True" />
        </record>

    </data>

</odoo>