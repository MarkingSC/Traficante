<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_move_search_view" model="ir.ui.view">
            <field name="inherit_id" ref="stock.view_move_search"></field>
            <field name="name">stock.move.search.view</field>
            <field name="model">stock.move</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="date" string="Desde" filter_domain="[('date', '&gt;=', self)]"/>
                    <field name="date" string="Hasta" filter_domain="[('date', '&lt;=', self)]"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>