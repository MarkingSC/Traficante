#. module: mb_authorizations
#: model:mail.template,body_html:mb_authorizations.authorization_assigned_notification_template
msgid ""
"<div>\n"
"                    <p>\n"
"                        Hello,\n"
"                        <br/>\n"
"                        <br/>\n"
"                        The user ${object.applicant_uid.name} have tried to "
"${object.name}. But it needs your authorization to continue with the "
"process.\n"
"                    </p>\n"
"                    <p>\n"
"                        Te reason is:\n"
"                        <br/>\n"
"                        ${object.reason}\n"
"                    </p>\n"
"                    Please take a look to the task:\n"
"                    <div style=\"text-align: center; margin: 16px 0px 16px "
"0px;\">\n"
"                        <a href=\"${object.task_url}\" style=\"background-"
"color:#875A7B;padding: 8px 16px 8px 16px; text-decoration: none; color: "
"#fff; border-radius: 5px; font-size:13px;\">\n"
"                            View\n"
"                        </a>\n"
"                    </div>\n"
"                    <br/>\n"
"                    Thank you,\n"
"                    <br/>\n"
"                    % if object.applicant_uid and object.applicant_uid."
"signature:\n"
"                        ${object.applicant_uid.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    <p>\n"
"                        Hola,\n"
"                        <br/>\n"
"                        <br/>\n"
"                        El usuario ${object.applicant_uid.name} ha "
"intentado ${object.name}. Pero necesita de su autorización para continuar "
"con el proceso.\n"
"                    </p>\n"
"                    <p>\n"
"                        La razón es:\n"
"                        <br/>\n"
"                        ${object.reason}\n"
"                    </p>\n"
"                    Por favor revise esta solicitud:\n"
"                    <div style=\"text-align: center; margin: 16px 0px 16px "
"0px;\">\n"
"                        <a href=\"${object.task_url}\" style=\"background-"
"color:#875A7B;padding: 8px 16px 8px 16px; text-decoration: none; color: "
"#fff; border-radius: 5px; font-size:13px;\">\n"
"                            Revisar\n"
"                        </a>\n"
"                    </div>\n"
"                    <br/>\n"
"                    Gracias,\n"
"                    <br/>\n"
"                    % if object.applicant_uid and object.applicant_uid."
"signature:\n"
"                        ${object.applicant_uid.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

